# Example of demo shop 'STORE SPECIFIC' data import for '{{ storeName }}' store.
version: 0

actions:
    # Store relation
    # Category
    - data_entity: category-store
      source: data/import/common/{{ storeName }}/category_store.csv
    # CMS pages
    - data_entity: cms-block-store
      source: data/import/common/{{ storeName }}/cms_block_store.csv
    - data_entity: cms-page-store
      source: data/import/common/{{ storeName }}/cms_page_store.csv
    - data_entity: product-option-price
      source: data/import/common/{{ storeName }}/product_option_price.csv
    # Discounts
    - data_entity: discount-amount
      source: data/import/common/{{ storeName }}/discount_amount.csv
    - data_entity: discount-store
      source: data/import/common/{{ storeName }}/discount_store.csv
    # Merchants
    - data_entity: product-price-merchant-relationship
      source: data/import/common/{{ storeName }}/price_product_merchant_relationship.csv
    # Payment methods
    - data_entity: payment-method-store
      source: data/import/common/{{ storeName }}/payment_method_store.csv
    # Product
    - data_entity: product-abstract-store
      source: data/import/common/{{ storeName }}/product_abstract_store.csv
    - data_entity: product-price
      source: data/import/common/{{ storeName }}/product_price.csv
    - data_entity: product-price-schedule
      source: data/import/common/{{ storeName }}/product_price_schedule.csv
    - data_entity: product-label-store
      source: data/import/common/{{ storeName }}/product_label_store.csv
    - data_entity: product-measurement-sales-unit-store
      source: data/import/common/{{ storeName }}/product_measurement_sales_unit_store.csv
    - data_entity: product-relation-store
      source: data/import/common/{{ storeName }}/product_relation_store.csv
    # Shipment methods
    - data_entity: shipment-method-store
      source: data/import/common/{{ storeName }}/shipment_method_store.csv
    - data_entity: shipment-price
      source: data/import/common/{{ storeName }}/shipment_price.csv
    # Sales Order Threshold
    - data_entity: sales-order-threshold
      source: data/import/common/{{ storeName }}/sales_order_threshold.csv
    - data_entity: merchant-relationship-sales-order-threshold
      source: data/import/common/{{ storeName }}/sales_order_threshold_per_merchant_relationship.csv
    # Stock
    - data_entity: stock-store
      source: data/import/common/{{ storeName }}/warehouse_store.csv
    # Cart
    - data_entity: multi-cart
      source: data/import/common/{{ storeName }}/multi_cart.csv
    - data_entity: shared-cart
      source: data/import/common/{{ storeName }}/shared_cart.csv
    # Comment
    - data_entity: comment
      source: data/import/common/{{ storeName }}/comment.csv
